  "scripts": {
    "build:gateway": "rm -rf dist/apps/gateway && nest build gateway",
    "build:manager-company": "rm -rf dist/apps/manager-company && nest build manager-company",
    "build:order-management": "rm -rf dist/apps/order-management && nest build order-management",
    "build:product": "rm -rf dist/apps/product && nest build product",
    "build:role": "rm -rf dist/apps/role && nest build role",
    "build:user": "rm -rf dist/apps/user && nest build user",
    "build:button-easy": "rm -rf dist/apps/button-easy && nest build button-easy",
    "build:backup": "rm -rf dist/apps/backup && nest build backup",
    "build:clear": "npm run lint && npm run prettier && npm run check-types && npm run build",
    "start:gateway": "nest start gateway --watch",
    "start:user": "nest start user --watch",
    "start:story": "nest start story --watch",
    "start:role": "nest start role --watch",
    "start:customer": "nest start customer --watch",
    "start:button-easy": "nest start button-easy --watch",
    "start:backup": "nest start backup --watch",
    "run:button-easy": "node dist/apps/button-easy/apps/button-easy/src/main.js",
    "run:manager-company": "node dist/apps/manager-company/apps/manager-company/src/main.js",
    "run:order-management": "node dist/apps/order-management/apps/order-management/src/main.js",
    "run:role": "node dist/apps/role/apps/role/src/main.js",
    "run:product": "node dist/apps/product/apps/product/src/main.js",
    "run:gateway": "node dist/apps/gateway/apps/gateway/src/main.js",
    "run:user": "node dist/apps/user/apps/user/src/main.js",
    "run:customer": "node dist/apps/customer/apps/customer/src/main.js",
    "run:backup": "node dist/apps/backup/apps/backup/src/main.js",
    "build:core": "nest build core",
    "build:shared": "nest build shared",
    "compodoc": "npx compodoc -p ./tsconfig.doc.json",
    "compodoc:watch": "npx compodoc -p ./tsconfig.doc.json -s",
    "lint": "eslint .",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier -c \"{src,apps,libs,test}/**/*.ts\" ",
    "prettier:fix": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "check-types": "tsc --pretty --noEmit",
    "pre-commit": "npm run lint && npm run prettier && npm run check-types",
    "prepare": "husky install"
  },
